<div class="col-xs-6 col-xs-offset-3">
<h1>Time to setup your Photo and Biography</h1>
<%= bootstrap_form_for(@user, :url => wizard_path, :method => :put) do |f| %>
  <%= f.text_field :avatar, :placeholder => "Avatar" %>
  <%= f.file_field :profile_photo %>
  <%= f.text_field :bio, :placeholder => "Biography", :maxlength=>"170" %>
  <%= f.submit 'Finish Profile', :class => 'btn btn-primary' %>
  <%= link_to 'skip', squawks_path, :class => "btn btn-default" %>
<% end %>


</div>
